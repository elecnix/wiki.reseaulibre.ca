"     config 'wifi-iface' 'mesh'\n"
"            option 'device' 'wifi0'\n"
"            option 'network' 'mesh'\n"
"            option 'mode' 'ap'\n"
"            option 'ssid' 'mesh'\n"
"            option 'hidden' '0'\n"
"            option 'encryption' 'none'\n"
