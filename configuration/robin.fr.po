#. type: Plain text
#, no-wrap
msgid ""
"     config iprules filter\n"
"            option SMTP_rdir       0\n"
"            option SMTP_dest       0.0.0.0\n"
"            option LAN_BLOCK       1\n"
"            option ICMP_BLOCK      0\n"
"            option AP1_bridge      1\n"
"            option AP2_bridge      1\n"
"            option MTU_CLAMP       0 \n"
"            option dashboard_IP    0.0.0.0\n"
"            option enable_log      0\n"
"            option log_server\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid ""
"     config iprules start\n"
"            option pre             S77\n"
"            option post            S79\n"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "     #\n"
msgstr ""

#. type: Bullet: '* '
msgid ""
"Sécuriser l'accès au LAN (192.168.1.* ou autre) à partir de l'AP ouvert à "
"tous:"
