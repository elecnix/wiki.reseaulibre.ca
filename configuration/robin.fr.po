#, no-wrap
msgid ""
"     config 'wifi-iface' 'private'\n"
"            option 'device' 'wifi0'\n"
"            option 'network' 'ap2'\n"
"            option 'mode' 'ap'\n"
"            option 'hidden' '0'\n"
"            option 'encryption' 'psk'\n"
"            option 'key' '0p3nm35h'\n"
"            option 'ssid' 'my open-mesh'\n"
"            option 'isolate' '1'\n"
