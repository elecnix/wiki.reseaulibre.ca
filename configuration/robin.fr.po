#. type: Plain text
#, no-wrap
msgid "     for IFACE in $AP1_iface $AP2_iface $WAN ; do\n"
msgstr ""

#. type: Plain text
msgid "Par"
msgstr ""

#. type: Plain text
#, no-wrap
msgid "    for IFACE in $AP1_iface $WAN ; do\n"
msgstr ""

#. type: Plain text
msgid "L'interface $AP2_iface étant l'interface privée..."
