"            option 'device' 'wifi0'\n"
"            option 'network' 'ap1'\n"
"            option 'mode' 'ap'\n"
"            option 'ssid' 'open-mesh'\n"
"            option 'hidden' '0'\n"
"            option 'encryption' 'none'\n"
"            option 'isolate' '1'\n"
